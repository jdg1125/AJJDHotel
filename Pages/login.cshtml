@page
@model IndexModel
@{
    ViewData["Title"] = "Login page";
}
<form name="login">
    <div class="login-box">
        <h1>Login</h1>
        <div class="textbox">
            <input type="text" placeholder="Username" name="" value="">
        </div>

        <div class="textbox">
            <input type="password" placeholder="Password" name="" value="">
        </div>

        <input type="submit" name="submit" value="signup" />

    </div>
</form>
<form name="signup" method="GET"
      action="process.php">

    <div class="Register-box">
        <h1>Register</h1>
        <div class="textbox">
            <label for="first-name">First Name:</label>
            <input type="text" placeholder="John" name="first-name" value="">
        </div>

        <div class="textbox">
            <label for="Last-name">Last name:</label>
            <input type="text" placeholder="Doe" name="Last-name" value="">
        </div>

        <div class="textbox">
            <label for="phone-number">phone number:</label>
            <input type="text" placeholder="555-555-5555" name="phone-number" value="">
        </div>

        <div class="textbox">
            <label for="birthday">birthday:</label>
            <input type="date" placeholder="" name="birthday" value="">
        </div>

        <div class="textbox">
            <label for="email">email:</label>
            <input type="email" name="email" id="email-address" placeholder="email@example.com" />
        </div>

        <div class="textbox">
            <label for="Password">password:</label>
            <input type="password" placeholder="password" name="Password" value="">
        </div>

        <input type="submit" name="submit" value="Register" />
</form>









</div>
